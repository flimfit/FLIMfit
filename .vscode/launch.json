{
   "version": "0.2.0",
   "configurations": [
      {
         "name": "(Windows) Attach",
         "type": "cppvsdbg",
         "request": "attach",
         "processId": "${command:pickProcess}"
      },
      {
         "name": "(lldb) Launch Test",
         "type": "cppdbg",
         "request": "launch",
         "program": "${workspaceRoot}/GeneratedProjects/Unix/FLIMfitLibrary/TestHarness/TestHarness",
//         "program": "${workspaceRoot}/GeneratedProjects/Unix/FLIMfitLibrary/FLIMreader/Source/FlimReaderTest/FlimReaderTest",
         "MIMode": "lldb",
         "cwd": "${workspaceRoot}/FLIMfitFrontEnd/",
         "preRunCommands": "process handle -p true -n false -s false SIGSEGV SIGBUS"
      },
      {
         "name": "(lldb) Launch Matlab",
         "type": "cppdbg",
         "request": "launch",
         "program": "/Applications/MATLAB_R2016b.app/Contents/MacOS/MATLAB_maci64",
         "args": ["-Dlldb"],
//         "processId": "${command:pickProcess}",
         "MIMode": "lldb",
         "cwd": "${workspaceRoot}/FLIMfitFrontEnd/",
         "preRunCommands": "process handle -p true -n false -s false SIGSEGV SIGBUS"
      },
      {
         "name": "(lldb) Attach to Matlab",
         "type": "cppdbg",
         "request": "attach",
         "program": "/Applications/MATLAB_R2016b.app/Contents/MacOS/MATLAB_maci64",
         "args": ["-Dlldb"],
         "processId": "${command:pickProcess}",
         "MIMode": "lldb",
         "cwd": "${workspaceRoot}/FLIMfitFrontEnd/",
         "preRunCommands": "process handle -p true -n false -s false SIGSEGV SIGBUS"
      }
   ]
}